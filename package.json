{
  "name": "nuxt-pwa",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "David Royer <droyer01@gmail.com>",
  "private": true,
  "dependencies": {
    "@nuxtjs/axios": "^3.1.4",
    "@nuxtjs/proxy": "^1.1.1",
    "@nuxtjs/pwa": "latest",
    "axios": "^0.16.2",
    "concurrently": "^3.5.0",
    "element-ui": "^1.4.1",
    "nuxt": "latest",
    "pm2": "^2.6.1"
  },
  "scripts": {
    "dev": "nuxt",
    "dev-api": "concurrently \"yarn api\" \"yarn dev\"",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "old": "concurrently --kill-others-on-fail \"yarn start\" \"yarn generate\"",
    "test1": "yarn start & yarn generate && exit",
    "package": "yarn pre-generate && yarn generate && yarn post-generate",
    "pre-generate": "yarn build && pm2 start npm --name \"nuxtapp\" -- start",
    "post-generate": "pm2 stop \"nuxtapp\"",
    "close": "exit",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "api": "firebase serve --only functions"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6"
  }
}
